<p id="notice"><%= notice %></p>

<h1>Branch Products</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Product</th>
      <th>Stock</th>
      <th>y</th>
      <th>x</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @branch_products.each do |branch_product| %>
      <tr>
        <td><%= Branch.find(branch_product.branch_id).name %></td>
        <td><%= Product.find(branch_product.product_id).name %></td>
        <td><%= branch_product.stock %></td>
        <td><%= branch_product.longitude %></td>
        <td><%= branch_product.latitude %></td>
        <td><%= link_to 'Show', branch_product %></td>
        <td><%= link_to 'Edit', edit_branch_product_path(branch_product) %></td>
        <td><%= link_to 'Destroy', branch_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<style>
  #circle {
	  visibility: visible;
  }

  #circle:hover + #label{
  	visibility: visible;
  }

  #label {
  	visibility: hidden;
  }
</style>


